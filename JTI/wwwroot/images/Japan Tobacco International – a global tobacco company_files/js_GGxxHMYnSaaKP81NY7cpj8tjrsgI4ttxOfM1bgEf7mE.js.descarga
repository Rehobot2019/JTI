jQuery(document).ready(function(){
	if((window.location.hash.substr(1).indexOf("$v") !== -1) && jQuery("#videoPlayer-share").length > 0)
	{	
		var videoArray = window.location.hash.substr(1).split('=');
		
		jQuery("#videoPlayer-share").attr("data-inv_videoid", videoArray[1]);
		
		jQuery("#videoPlayer-share").InvPlayer({
		    onPlayerReady: function() {
		   		    $.magnificPopup.open({
				      items: {
				        type: 'inline',        
				        preloader: false,
